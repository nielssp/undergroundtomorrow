{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es2015",
    "module": "ESNext",
    "strict": true,
    "sourceMap": true,
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "jsxFactory": "createElement",
    "jsxFragmentFactory": "Fragment",
    "moduleResolution": "node"
  }
}
